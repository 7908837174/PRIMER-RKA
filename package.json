{
  "name": "primer-rka",
  "version": "1.0.0",
  "description": "PRIMER - Smart Group Project Collaborator",
  "scripts": {
    "install:backend": "cd Backend1 && npm install",
    "install:client": "cd client && npm install",
    "install:all": "npm run install:backend && npm run install:client",
    "build:backend": "cd Backend1 && npm run build",
    "build:client": "cd client && npm run build",
    "build": "npm run build:backend && npm run build:client",
    "start:backend": "cd Backend1 && npm start",
    "start:client": "cd client && npm run preview",
    "dev:backend": "cd Backend1 && npm run dev",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:client\""
  },
  "keywords": [
    "project-management",
    "collaboration",
    "ai",
    "mern-stack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}