{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run install:all && npm run build:client",
        "outputDirectory": "client/dist"
      }
    },
    {
      "src": "Backend1/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend1/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/dist/$1"
    }
  ],
  "functions": {
    "Backend1/api/index.js": {
      "maxDuration": 30
    }
  }
}